function GetVoiceForm(Sound[] TargetVoice, int TargetFormID, int size)
    int index = 0

    While (index < size)
        TargetVoice[index] = Game.GetFormFromFile(TargetFormID, "SLHGameVoicesNonStop.esp") as Sound
        index += 1
        TargetFormID += 1
    endWhile
endfunction


