function PlayVoice(int ThreadID)
    sslThreadController Controller = SexLab.GetController(ThreadID)                             ;Get Thread Controller from Thread
    actor[] actorList = Controller.Positions                                                    ;Get ActorList array from Thread Controller
    int actorLength = actorList.Length                                                         ;Get length of List

    int i = 0                                                                                   ;i = Current Actor Number
                                                                                                ;actorList[i] = TargetActor
    While i < actorLength
        int arrayIndex = i + (ThreadId * 5)

        sslBaseVoice BaseVoice = SexLab.GetSavedVoice(actorList[i])                             ;Get BaseVoice of actor
        string actorVoiceName = BaseVoice.Name                                                  ;Get BaseVoice Name
        bool IsActorSMVoice = CheckSMVoice(actorVoiceName)                                      ;Check Actor is SMVoice or not

        if IsActorSMVoice
            Controller.SetVoice(actorList[i], BaseVoice, true)

            sslBaseExpression Expression = Controller.GetExpression(actorList[i])               ;Get Expression From Controller
            string actorExpressionName = Expression.Name

            if savedActorVName[arrayIndex] != actorVoiceName                                    ;When Voice Changed (or Start)
                if actorPlayingFlag[arrayIndex] == 1

                endIf
                actorPlayingFlag[arrayIndex] = 0
                savedActorVName[arrayIndex] = actorVoiceName
            endIf

            if savedActorExpression[arrayIndex] != actorExpressionName                          ;Or Expression changed
                if actorPlayingFlag[arrayIndex] == 1
                    Sound.StopInstance(actorInstanceID[arrayIndex])                             ;Stop playing voice to change type
                endIf
                actorPlayingFlag[arrayIndex] = 0 
                savedActorExpression[arrayIndex] = actorExpressionName
            endIf

            if Controller.SoundFX == Sucking || Controller.SoundFX == Mixed                     ;If Stage = sucking
                if !IsActorOral[arrayIndex]                                                     ;Check flag oral = is not oral before
                    Sound.StopInstance(actorInstanceID[arrayIndex])                             ;Stop & set playing flag 0
                    actorPlayingFlag[arrayIndex] = 0;
                endIf
                IsActorOral[arrayIndex] = true                                                  ;Change flag true to playing oral
            else
                if IsActorOral[arrayIndex]                                                      ;If stage is not sucking
                    Sound.StopInstance(actorInstanceID[arrayIndex])                             ;Stop playing voice and set playing flag 0
                    actorPlayingFlag[arrayIndex] = 0;
                endIf
                IsActorOral[arrayIndex] = false                                                 ;Set oral flag to false to play normal voice
            endIf

            actorEnjoyment[arrayIndex] = Controller.GetEnjoyment(actorList[i])                  ;Get enjoyment from ThreadController

            if voiceTypePlaying[ThreadID] == 1                                                  ;If Playing not comming voice
                if actorRelease[arrayIndex] > 0                                                 ;Setting for Playing Time Algorithm
                    actorFtimeStart[arrayIndex] = Utility.GetCurrentRealTime()
                    
                    if actorHold[arrayIndex] == 0                                               ;If Hold statement=0, then start timer and set Hold to 1
                        actorTimer[arrayIndex] = actorFtimeStart[arrayIndex]                      
                        actorHold[arrayIndex] = 1
                    endIf
    
                    if actorFtimeStart[arrayIndex] >= actorTimer[arrayIndex] + actorRelease[arrayIndex] - 1
                        actorPlayingFlag[arrayIndex] = 0
                        actorRelease[arrayIndex] = 0
                        Sound.StopInstance(actorInstanceID[arrayIndex])
                    endIf
                endIf
    
                if actorEnjoyment[arrayIndex] >= 0 && actorEnjoyment[arrayIndex] <= 20          ;Check Enjoyment and set state
                    actorNewstate[arrayIndex] = 1
                elseIf actorEnjoyment[arrayIndex] >= 21 && actorEnjoyment[arrayIndex] <= 40
                    actorNewstate[arrayIndex] = 2
                elseIf actorEnjoyment[arrayIndex] >= 41 && actorEnjoyment[arrayIndex] <= 60
                    actorNewstate[arrayIndex] = 3
                elseIf actorEnjoyment[arrayIndex] >= 61 && actorEnjoyment[arrayIndex] <= 99
