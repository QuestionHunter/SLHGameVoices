                endIf
            
                if IsActorOral[arrayIndex]                                                      ;If actor is Oral Play Oral Voice, else Play normal voice
                    PlayOralVoice(actorVoiceName, actorList[i], arrayIndex, actorExpressionName)
                else
                    PlayNormalVoice(actorVoiceName, actorList[i], arrayIndex, actorExpressionName)
                endIf
            endIf
