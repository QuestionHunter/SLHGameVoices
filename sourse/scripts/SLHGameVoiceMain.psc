event OnInit()
    ;Debug.Notification("SLSMVoice On")                                              ;Notice Voice Addon start when game start
    RegisterForModEvent("AnimationStart", "PlayerSex")                              ;Register Event
    RegisterForModEvent("OrgasmStart", "Cuming")
    RegisterForModEvent("StageStart", "Again")
    RegisterForModEvent("AnimationEnd", "Done")

    voiceTypePlaying = new int[15]                                                  ;To Use on Voice State
    IsActorOral = new bool[75]                                                      ;Save Is actor State is Oral Now
    savedActorVName = new string[75]                                                ;Save Actor Voice Name to compare
    savedActorExpression = new string[75]                                           ;Save Actor Expression Name to compare
    actorInstanceID = new int[75]                                                   ;Save Sound Instance ID
    actorPlayingFlag = new int[75]                                                  ;Save Voice Playing Type flag 0, 1, 2
    actorEnjoyment = new int[75]                                                    ;Save Actor's Enjoyment
    actorRelease = new int[75]
    actorTimer = new float[75]
    actorFtimeStart = new float[75]
    actorOldstate = new int[75]
    actorNewstate = new int[75]
    actorHold = new int[75]
    InitVoice()
endEvent


event PlayerSex(string eventName, string argString, float argNum, form sender)      ;When Sex start
    Sucking = Game.GetFormFromFile(0x65A32, "SexLab.esm") as Sound                  ;Get Sucking Voice from SexLab.esm for check animation is oral
    Mixed = Game.GetFormFromFile(0x65A33, "SexLab.esm") as Sound                    ;Get Mixed Voice
    tid = argString as int                                                          ;Get Thread ID
    ThreadSize = argString as int + 1                                               ;Thread Size

    int i = ThreadSize
    While i
        i -= 1
        voiceTypePlaying[i] = 1
        IsAnimating = true
    endWhile

    RegisterForSingleUpdate(1.0)
endEvent


event OnUpdate()
    if IsAnimating
        RegisterForSingleUpdate(0.5)
    endIf

    int i = ThreadSize
    While i
        i -= 1
        PlayVoice(i)
    endWhile
endEvent


event Again(string eventName, string argString, float argNum, form sender)
    int i = ThreadSize
    while i
        i -= 1
        actor[] actorList = SexLab.GetController(i).Positions
        sslThreadController ControllerActor = SexLab.GetActorController(actorList[0])

        int StageCount = ControllerActor.Animation.StageCount
        int Stage = SexLab.GetController(i).Stage

        if Stage != StageCount
            voiceTypePlaying[i] = 1
        endIf
    endWhile
endevent


event Cuming(string eventName, string argString, float argNum, form sender)
    int ThreadId = ThreadSize
    while ThreadId
        ThreadId -= 1

        voiceTypePlaying[ThreadId] = 2
        actor[] actorList = SexLab.GetController(ThreadId).Positions
        int actorLength = actorList.Length

        int actorNum = 0
        while actorNum < actorLength
            int arrayIndex = actorNum + (ThreadId * 5)
            string actorVoiceName = SexLab.GetSavedVoice(actorList[actorNum]).Name
            string actorExpressionName = SexLab.GetController(ThreadID).GetExpression(actorList[actorNum]).Name

            Sound.StopInstance(actorInstanceID[arrayIndex])
            CumingVoice(actorVoiceName, actorList[actorNum], arrayIndex, actorExpressionName)
            actorRelease[arrayIndex] = 7
            actorNum += 1
        endWhile
    endWhile
endEvent


event Done(string eventName, string argString, float argNum, form sender)
    int ThreadId = argString as int
        
    actor[] actorList = SexLab.GetController(ThreadId).Positions
    int actorLength = actorList.Length

    int actorNum = 0
    while actorNum < actorLength
        int arrayIndex = actorNum + (ThreadId * 5)

        savedActorVName[arrayIndex] = ""
        savedActorExpression[arrayIndex] = ""
        actorInstanceID[arrayIndex] = 0
        actorPlayingFlag[arrayIndex] = 0
        actorEnjoyment[arrayIndex] = 0
        actorRelease[arrayIndex] = 0
        actorTimer[arrayIndex] = 0
        actorFtimeStart[arrayIndex] = 0
        actorOldstate[arrayIndex] = 0
        actorNewstate[arrayIndex] = 0
        actorHold[arrayIndex] = 0
        
        actorNum += 1
    endWhile
    voiceTypePlaying[ThreadId] = 0
endEvent


