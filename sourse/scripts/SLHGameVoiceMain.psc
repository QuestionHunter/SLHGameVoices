bool function CheckSMVoice(string Name)
    if Name == "Voice_Yui" || Name == "Voice_Rion" || Name == "Voice_Miyuki" || Name == "Voice_Shizuku"|| Name == "Voice_Kagami" || Name == "Voice_Mizuki" || Name == "Voice_Akari" || Name == "Voice_Suzuka" || Name == "AG3_TypeS" || Name == "AG3_TypeA" || Name == "AG3_TypeC" || Name == "AG3_TypeJ" || Name == "AG3_TypeK" || Name == "AG3_TypeM" || Name == "AG3_TypeN" || Name == "AG3_TypeP" || Name == "AG3_TypeT" || Name == "AG3_TypeU" || Name == "AG3_TypeV" || Name == "AG3_TypeY" || Name == "HS2_C00_Composed"|| Name == "HS2_C01_Normal"|| Name == "HS2_C02_Hardworking"|| Name == "HS2_C03_Girlfriend"|| Name == "HS2_C04_Fashionable"|| Name == "HS2_C05_Timid"|| Name == "HS2_C06_Motherly"|| Name == "HS2_C07_Sadistic"|| Name == "HS2_C08_OpenMinded"|| Name == "HS2_C09_Airhead"|| Name == "HS2_C10_Careful"|| Name == "HS2_C11_IdealJapanese"|| Name == "HS2_C12_Tomboy"|| Name == "HS2_C13_Obsessed"
        return true
    else
